; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Mining Space"
config/tags=PackedStringArray("developing", "game")
run/main_scene="res://src/main_level.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=193
window/size/viewport_height=344
window/size/window_width_override=386
window/size/window_height_override=688
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/handheld/orientation=1

[layer_names]

2d_physics/layer_1="player"
2d_physics/layer_2="asteroids"
2d_physics/layer_3="lasers"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
